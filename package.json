    "eslint-plugin-promise": "^6.6.0",
    "jest-cli": "^29.7.0",
    "lodash": "^4.17.21",
    "marked": "^15.0.12",
    "rollup": "^4.29.2",
    "semantic-release": "^24.2.0"
